{
  "name": "kkalla-api",
  "private": true,
  "packageManager": "pnpm@10.1.0",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -b swc",
    "start:dev": "nest start -b swc --watch",
    "start:debug": "nest start -b swc --watch --debug",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test,migrations}/**/*.ts\" --fix",
    "prettier": "prettier \"{src,test,migrations}/**/*.ts\" --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "pnpm typeorm migration:create ./src/databases/migrations/migration",
    "migration:generate": "pnpm typeorm -- migration:generate ./src/databases/migrations/migration -d ./src/databases/datasource.ts",
    "migration:run": "pnpm typeorm -- migration:run -d ./src/databases/datasource.ts",
    "migration:revert": "pnpm typeorm -- migration:revert -d ./src/databases/datasource.ts"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.947.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@slack/web-api": "^7.13.0",
    "@swc/helpers": "^0.5.17",
    "ccxt": "^4.5.26",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "google-auth-library": "^10.5.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.8.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.15.3",
    "nestjs-i18n": "^10.6.0",
    "openai": "^6.10.0",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "redlock": "5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.28",
    "typeorm-encrypted": "^0.8.0",
    "typeorm-extension": "^3.7.2",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@jest/test-sequencer": "^30.2.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "source-map-support": "^0.5.21",
    "sql.js": "^1.13.0",
    "supertest": "^7.1.4",
    "@swc/jest": "^0.2.37",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "@swc/jest",
        {
          "sourceMaps": true,
          "module": {
            "type": "commonjs",
            "strict": true
          },
          "jsc": {
            "target": "es2022",
            "parser": {
              "syntax": "typescript",
              "decorators": true
            },
            "transform": {
              "legacyDecorator": true,
              "decoratorMetadata": true
            },
            "keepClassNames": true
          }
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "ccxt",
      "@swc/core",
      "@nestjs/core",
      "ccxt",
      "@swc/core",
      "@nestjs/core"
    ],
    "onlyBuiltDependencies": [
      "ccxt",
      "@swc/core",
      "@nestjs/core"
    ]
  }
}
